@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Błąd.</h1>
<h2 class="text-danger">Wytąpił błąd podczas przetwarzania strony.</h2>

<h3 class="text-danger">@Model.ActionName</h3>
<h3 class="text-danger">@Model.ControllerName</h3>
<h3 class="text-danger">@Model.Exception.ToString()</h3>
