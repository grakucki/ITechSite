@model IEnumerable<ITechSite.Models.Resource>

@{
    ViewBag.Title = "View";
}

<h2>View</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.No)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WorkProcess)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Enabled)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResourceType.Type)
        </th>
         <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Model.Car)
        </th>*@
        @*<th>
            @Html.DisplayNameFor(model => model.Workstation.WorkstationGroup)
        </th>*@
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.No)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WorkProcess)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Enabled)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Model.Car)
        </td>*@
        @*<td>
            @Html.DisplayFor(modelItem => item.Workstation.WorkstationGroup)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.ResourceType.Type)
        </td>
        <th>
            @Html.DisplayFor(modelItem => item.Description)
        </th>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
